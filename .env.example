# Wavenet Care - Environment Variables Configuration
# Copy this file to .env.local and update the values

# API Configuration
# For local development, use: http://localhost:8000/api
# For production, use your deployed backend URL: https://your-api.com/api
NEXT_PUBLIC_API_URL=http://localhost:8000/api

# Application Name
NEXT_PUBLIC_APP_NAME=WavenetCare

# Mock Service Worker (MSW) for Development
# Set to 'true' to enable API mocking in development
# Set to 'false' or remove to use real API
NEXT_PUBLIC_MSW_ENABLED=false

# Optional: Analytics
# NEXT_PUBLIC_GA_ID=your-google-analytics-id

# Optional: Sentry Error Tracking
# NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn
